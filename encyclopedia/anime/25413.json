    "episode": [
        {
            "+@num": "1",
            "title": {
                "+content": "The strongest principle of growth lies in the human choice.",
                "+@gid": "2165933386",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "2",
            "title": {
                "+content": "Man is wolf to man.",
                "+@gid": "2770120625",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "3",
            "title": {
                "+content": "We never forget what we endeavor to forget.",
                "+@gid": "3133717363",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "4",
            "title": {
                "+content": "To work you have the right, but not to the fruits thereof.",
                "+@gid": "1106975012",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "5",
            "title": {
                "+content": "Fortune favors the bold.",
                "+@gid": "2610362703",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "6",
            "title": {
                "+content": "It is better to receive an injury than to inflict one.",
                "+@gid": "3029190440",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "7",
            "title": {
                "+content": "People will do anything, no matter how absurd, in order to avoid facing their own souls.",
                "+@gid": "2352882608",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "8",
            "title": {
                "+content": "Those who cannot remember the past are condemned to repeat it.",
                "+@gid": "99918356",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "9",
            "title": {
                "+content": "Extreme justice is extreme injustice.",
                "+@gid": "1569679893",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "10",
            "title": {
                "+content": "The first cause of Absurd conclusions I ascribe to the want of Method.",
                "+@gid": "3570819977",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "11",
            "title": {
                "+content": "There is only one rule in love: bring happiness to those you love.",
                "+@gid": "2685284094",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "12",
            "title": {
                "+content": "Change your desires rather than the order of the world.",
                "+@gid": "1902279480",
                "+@lang": "EN"
            }
        },
        {
            "+@num": "13",
            "title": {
                "+content": "Love is the best teacher.",
                "+@gid": "1779765638",
                "+@lang": "EN"
            }
        }
    ],

    "episode": [
        {
            "+@num": "1",
            "title": [
                {
                    "+content": "Boy ♂ Meets ♀ Girl I shall have your \"first time\"!!",
                    "+@part": "A",
                    "+@gid": "2103760032",
                    "+@lang": "EN"
                },
                {
                    "+content": "The grand operation after school! In any case let's kiss for a start?",
                    "+@part": "B",
                    "+@gid": "3834145881",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "2",
            "title": [
                {
                    "+content": "That's it! Let's go to the pool. You want to see my swimsuit don't you!",
                    "+@part": "A",
                    "+@gid": "516332198",
                    "+@lang": "EN"
                },
                {
                    "+content": "Rival appears. Who? That one with the F-cup!?",
                    "+@part": "B",
                    "+@gid": "3988239097",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "3",
            "title": [
                {
                    "+content": "Closing the distance in the dark room! Club activity that's a little dangerous!?",
                    "+@part": "A",
                    "+@gid": "2935583204",
                    "+@lang": "EN"
                },
                {
                    "+content": "Cultural festival all fired up! The queen of the school is me!!",
                    "+@part": "B",
                    "+@gid": "2922799322",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "4",
            "title": [
                {
                    "+content": "Heart-throbbing Christmas Eve. What's the taste of the first kiss?",
                    "+@part": "A",
                    "+@gid": "927118400",
                    "+@lang": "EN"
                },
                {
                    "+content": "We won't be going home if this goes on! Let's go to the lovers' park!",
                    "+@part": "B",
                    "+@gid": "2285626495",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "5",
            "title": [
                {
                    "+content": "A Valentine of sweat and tears. With love, from Yamada!",
                    "+@part": "A",
                    "+@gid": "2015930932",
                    "+@lang": "EN"
                },
                {
                    "+content": "Polish the Ero Powers! For the first time... such a sensation",
                    "+@part": "B",
                    "+@gid": "2026240407",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "6",
            "title": [
                {
                    "+content": "Kanejō comes! I can't forgive that glitter!",
                    "+@part": "A",
                    "+@gid": "3352980339",
                    "+@lang": "EN"
                },
                {
                    "+content": "Behind that rosy smiling face... the dark secret of Kanejō!!",
                    "+@part": "B",
                    "+@gid": "1805707316",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "7",
            "title": [
                {
                    "+content": "Showdown in school swimsuits! I'll lose to anyone but you!",
                    "+@part": "A",
                    "+@gid": "3141251884",
                    "+@lang": "EN"
                },
                {
                    "+content": "Memories of summer days... Yamada, returns to the wild!",
                    "+@part": "B",
                    "+@gid": "1788102396",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "8",
            "title": [
                {
                    "+content": "Yay, it's the school trip! There's no opportunity to be alone for the two of us...",
                    "+@part": "A",
                    "+@gid": "3693014927",
                    "+@lang": "EN"
                },
                {
                    "+content": "Everything about Yamada is so... confusing in this trouble-filled journey!",
                    "+@part": "B",
                    "+@gid": "2111007623",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "9",
            "title": [
                {
                    "+content": "How can this be! In front of everyone... Well, I didn't say anything about not liking him...",
                    "+@part": "A",
                    "+@gid": "3810400855",
                    "+@lang": "EN"
                },
                {
                    "+content": "I'll bet it all at the sports festival! It'll be alright as long as I make him like me...",
                    "+@part": "B",
                    "+@gid": "1879782629",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "10",
            "title": [
                {
                    "+content": "Kanejō's family. The glittering secrets of the celebrity brother!",
                    "+@part": "A",
                    "+@gid": "1557744354",
                    "+@lang": "EN"
                },
                {
                    "+content": "On the way to love? Keep your hands off my Kosuda!!",
                    "+@part": "B",
                    "+@gid": "1550131846",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "11",
            "title": [
                {
                    "+content": "Year 2 Class H's Christmas Eve. Take me to the bed",
                    "+@part": "A",
                    "+@gid": "310468141",
                    "+@lang": "EN"
                },
                {
                    "+content": "Kanejo is watching!? A \"live\" broadcast that is almost over the edge!",
                    "+@part": "B",
                    "+@gid": "2332699752",
                    "+@lang": "EN"
                }
            ]
        },
        {
            "+@num": "12",
            "title": [
                {
                    "+content": "The world revolves for us. Farewell... B Gata H Kei",
                    "+@part": "A",
                    "+@gid": "2565690890",
                    "+@lang": "EN"
                },
                {
                    "+content": "Yamada who became an angel! Farewell! B Gata H Kei!!",
                    "+@part": "B",
                    "+@gid": "1874927846",
                    "+@lang": "EN"
                }
            ]
        }
